<?xml version="1.0" encoding="UTF-8"?>
<mappings xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xmlns="http://dozer.sourceforge.net"
          xsi:schemaLocation="http://dozer.sourceforge.net
          http://dozer.sourceforge.net/schema/beanmapping.xsd">
    <configuration>
        <custom-converters>
            <converter type="com.zqkh.common.dozer.StringIdentifierConverter">
                <class-a>com.jovezhao.nest.ddd.Identifier</class-a>
                <class-b>java.lang.String</class-b>
            </converter>
            <converter type="com.zqkh.common.dozer.LocalDateTimeToDateDozerConverter">
                <class-a>java.time.LocalDateTime</class-a>
                <class-b>java.util.Date</class-b>
            </converter>
            <converter type="com.zqkh.common.dozer.LocalDateToDateDozerConverter">
                <class-a>java.time.LocalDate</class-a>
                <class-b>java.util.Date</class-b>
            </converter>
        </custom-converters>
    </configuration>

</mappings>